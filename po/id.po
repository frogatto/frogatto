#: data/objects/effects/control/level_controller_coin_race.cfg:21
msgid "You made it to level {level.player.difficulty} of the coin race."
msgstr "Kamu telah mencapai level {level.player.difficulty} pada balap koin."

#: data/objects/effects/control/level_controller_coin_race.cfg:22
msgid ""
"The furthest that has been reached is level {registry."
"arcade_coin_race_high_score}."
msgstr ""
